//Demonstrate execution of control flow, arithmetic and logical operations

class GRADES()
{
	def calc_GPA( points : Int, max : Int ) : Int
	{
	  points.PRINT();
		max.PRINT();
		GPA = 0;
		score = 0;
		score =  ( points * 100 ) / max ; 
		score.PRINT();
		if( score >= 95 )
		{
			GPA = 4;
			//return GPA;
		}
		elif( score < 95 )
		{
			if ( score >= 80 )
			{
				GPA = 3;
			}
			else
			{
				GPA = 2;
			}
		}
//		else
//		{
//			GPA = 1;
//		//	return GPA;
//		}
		GPA.PRINT();
		return GPA;
	}
}

x = 99;

while (x <= 50) {
	GRADES().calc_GPA(x,100).PRINT(); //GPA = 4, 3, 2, 1
	x = x - 10;
}
